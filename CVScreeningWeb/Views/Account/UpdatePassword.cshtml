@using CVScreeningWeb.Helpers
@using CVScreeningWeb.Resources
@model CVScreeningWeb.ViewModels.Account.AccountUpdatePasswordViewModel
@{
    ViewBag.Title = Account.ChangeMyPassword;
    ViewBag.PanelClass = CSSHelper.SINGLE_COLUMN_FORM_CLASS;
}


@Html.ValidationSummary(true)

<p style="text-align: center">@Account.LoggedInAs <strong>@User.Identity.Name</strong>.</p>


@Html.Partial("_ChangePasswordPartial")


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}