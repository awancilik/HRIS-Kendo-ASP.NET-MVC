
@using Microsoft.Ajax.Utilities
@model CVScreeningWeb.ViewModels.Report.AppendixReportViewModel
           
<h2>@Model.AppendixTitle</h2>
<h3>@Model.AppendixTypeOfCheck</h3>


@for(var i = 0; i < @Model.Attachments.Count(); i++)
{
    <strong>@Model.Attachments.ToArray()[i].FileName</strong>
    <br/><br/>
    <div style="width: 100%; height: 100%;">
        <img src="@Model.Attachments.ToArray()[i].ImagePath" style="width: 100%;" />
    </div>
    <div class="pdf-page-breaker"></div>
}

